.album-art-{{ album.UURL|raw }} {
    background: url('../{{ album.cover.webPath|raw }}') center center;
}

.album-{{ album.UURL|raw }}.album-details .row:first-child {
    background-color: {{ album.backgroundColor|raw }};
}

.album-{{ album.UURL|raw }} h3,
.album-{{ album.UURL|raw }} p,
.album-{{ album.UURL|raw }} span {
    color: {{ album.primaryColor|raw }};
}

.album-{{ album.UURL }} h3 {
    border-color: {{ album.secondaryColor|raw }};
}

.album-{{ album.UURL|raw }}.album-details .row:first-child .play-btn .pp-icon,
.album-{{ album.UURL|raw }}.album-details .row:first-child .highlight,
.album-{{ album.UURL|raw }}.album-details .row:first-child a,
.album-{{ album.UURL|raw }}.album-details .row:first-child a:active,
.album-{{ album.UURL|raw }}.album-details .row:first-child a:visited,
.album-{{ album.UURL|raw }}.album-details .row:first-child a:hover {
    color: {{ album.secondaryColor|raw }};
}

.album-{{ album.UURL|raw }} .album-list .album-list-toggle {
    border-bottom: 3px solid {{ album.primaryColor|raw }};
}

.album-{{ album.UURL|raw }} .play-btn .fa-circle,
.album-{{ album.UURL|raw }} .album-list .list-group-item .badge,
.album-{{ album.UURL|raw }} .album-list .list-group-item .track-number {
    color: {{ album.secondaryColor|raw }};
}