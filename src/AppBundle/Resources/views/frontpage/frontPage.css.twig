.profile .header {
    background: #fff url('../{{ frontPage.cover.webPath|raw }}') no-repeat center center;
}
.profile .header .img-div {
    background: #fff url('../{{ frontPage.image.webPath|raw }}') no-repeat center center;

}
.profile .header,
.profile .header .img-div {
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
}

